#include<bits/stdc++.h>
using namespace std;
 int main()
 {
 	vector<int> s;
 	string p;
 	cin>>p;
 	
 	int len=p.length();
 	
 	for(int i=0;i<len;i++)
 	{
 		s.push_back(p[i]);
	 }
 	int c,x=0;
 	for(int i=97;i<=122;i++)
 	{
 		c=count(s.begin(),s.end(),i);
 		if(c!=0)
 		{
 			x++;
		 }
	 }
	 
	 if(x%2==0)
	 {
	 	cout<<"CHAT WITH HER!";
	 }
	 else
	 {
	 	cout<<"IGNORE HIM!";
	 }
 }
