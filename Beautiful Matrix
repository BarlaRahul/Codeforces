#include<bits/stdc++.h>
using namespace std;
int main()
{
	int a[6][6];
	for( int i=1;i<6;i++)
	{
		for(int j=1;j<6;j++)
		{
			cin>>a[i][j];
		
		}
	}
	
	int p=0,q=0;
		for( int i=1;i<6;i++)
	{
		for( int j=1;j<6;j++)
		{
		
			if(a[i][j]==1)
			{
				p=abs(i-3);
				 q=abs(j-3);
				
				break;
			}
		}
		if(p!=0 || q!=0)
		{
			break;
		}
	}
 
    cout<<p+q;
    
}
