#include<iostream>
#include<string>
using namespace std;
int main()
{
	
	int p=0,i,n,r=0;
	string str;
	cin>>str;
	n=str.length();
	for(i=0;i<n;i++)
	{
		if(str[i]==str[i+1])
		{
			p=p+1;
			if(p>=6)
			{
				r=1;
				break;
			}
		}
		else
		{
			p=0;
		}
	}
	if(r==1)
	{
		cout<<"YES";
	}
	else
	{
		cout<<"NO";
	}
 
	
		
		
	
		return 0;
}
